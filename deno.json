{
  "tasks": {
    "compile": "deno compile --allow-run --allow-read --allow-env --allow-net --allow-write --unstable-kv --output dist/ax main.ts",
    "build-linux": "deno compile --target x86_64-unknown-linux-gnu --allow-run --allow-read --allow-env --allow-net --allow-write --unstable-kv --output dist/ax main.ts",
    "build-linux-arm64": "deno compile --target aarch64-unknown-linux-gnu --allow-run --allow-read --allow-env --allow-net --allow-write --unstable-kv --output dist/ax main.ts",
    "build-windows": "deno compile --target x86_64-pc-windows-msvc --allow-run --allow-read --allow-env --allow-net --allow-write --unstable-kv --output dist/ax.exe main.ts",
    "build-darwin": "deno compile --target aarch64-apple-darwin --allow-run --allow-read --allow-env --allow-net --allow-write --unstable-kv --output dist/ax main.ts",
    "test": "deno test --unstable --allow-write --allow-read"
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.7",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7",
    "@cliffy/table": "jsr:@cliffy/table@^1.0.0-rc.7"
  }
}
